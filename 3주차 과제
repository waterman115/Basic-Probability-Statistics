        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "x=np.linspace(-5,5,300) #-5에서5까지 300구간으로 나눔\n",
        "y1=x**2\n",
        "y2=(x-2)**2\n",
        "\n",
        "plt.plot(x,y1,color='r')\n",
        "plt.plot(x,y2,color='k',linestyle='--')\n",
        "plt.grid()\n",
        "plt.show()"
